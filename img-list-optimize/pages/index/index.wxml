<!--index.wxml-->
<view class="container">
  <view wx:for="{{itemList}}"
        class="list-item">
    <swiper style="height: 320px;">
      <swiper-item wx:for="{{imgList}}"
                   class="swiper-item">
        <image src="{{item.url}}" class="swiper-item" mode="widthFix"/>
      </swiper-item>
    </swiper>
    <text>{{'第 ' + (index + 1) + ' 个列表元素'}}</text>
  </view>
</view>
